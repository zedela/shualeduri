#include <stdio.h>

int main() {
    int C[5][5];
    int B[5];
    int max_value, max_row, max_col;
    
    // Input elements for the C array from the console
    printf("Enter elements for the C[5][5] array:\n");
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            printf("C[%d][%d]: ", i, j);
            scanf("%d", &C[i][j]);
        }
    }

    // Find the maximum element of the non-main diagonal and its indices
    max_value = C[0][4];
    max_row = 0;
    max_col = 4;
    for (int i = 1; i < 5; i++) {
        int j = 4 - i;
        if (C[i][j] > max_value) {
            max_value = C[i][j];
            max_row = i;
            max_col = j;
        }
    }

    // Copy the first column of C to the column where the maximum element is found
    for (int i = 0; i < 5; i++) {
        C[i][max_col] = C[i][0];
    }

    // Copy the row where the maximum element is found to B
    for (int i = 0; i < 5; i++) {
        B[i] = C[max_row][i];
    }

    // Print the modified C and B arrays
    printf("\nModified C[5][5] array:\n");
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            printf("%d\t", C[i][j]);
        }
        printf("\n");
    }

    printf("\nModified B[5] array:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d\t", B[i]);
    }
    
    return 0;
}
